{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "index.json",
  "title": "Meta index",
  "description": "Filesystem index file",
  "type": "object",
  "properties": {
    "files": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "dirs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["files", "dirs"]
}
