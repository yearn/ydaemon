{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "tokenList.json",
  "title": "TokenList",
  "description": "TokenList for Yearn ecosystem",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "timestamp": {
      "type": "string"
    },
    "logoURI": {
      "type": "string"
    },
    "version": {
      "type": "object",
      "properties": {
        "major": {
          "type": "number"
        },
        "minor": {
          "type": "number"
        },
        "patch": {
          "type": "number"
        }
      }
    },
    "tokens": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "chainId": { "type": "number" },
          "address": { "type": "string" },
          "name": { "type": "string" },
          "decimals": { "type": "number" },
          "symbol": { "type": "string" },
          "logoURI": { "type": "string" },
          "tags": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    }
  },
  "required": []
}
